    "ethereumjs-util": "^6.1.0",
    "ganache-cli": "^6.4.4",
    "openzeppelin-test-helpers": "^0.4.0",
    "solc": "^0.5.10",
    "solidity-coverage": "^0.5.11",
    "truffle": "^5.0.26",
    "truffle-hdwallet-provider": "^1.0.14",
    "ts-mocha": "^6.0.0",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.3",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "standard": {
